{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    {
      "identifier": "fs:allow-app-read-recursive",
      "allow": ["$APPDATA/**", "$RESOURCE/**"]
    },
    {
      "identifier": "fs:allow-app-write-recursive",
      "allow": ["$APPDATA/**", "$RESOURCE/**"]
    },
    "sql:default",
    "sql:allow-execute"
  ]
}
